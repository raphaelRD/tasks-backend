pipeline{
    agent any
    stages{
        stage('Inicio'){
            steps {
                bat 'echo inicio'
            }
        }
        stage('Meio'){
            steps {
                bat 'echo Meio'
            }
        }
        stage('Fim'){
            steps{
                sleep(5)
                bat 'echo Fim'
            }
        }
    }
    
    
}